<Dialog xmlns="sap.m" xmlns:core="sap.ui.core" title="{i18n>SHOW_SETTING}" contentWidth="400px" contentHeight="180px" class="showdialog">

    <content>
        <Label text="{i18n>SHOW_INS}"></Label>
        <Input id="showInput" value="{showJson>/RetrieveMaxHit/MaxHitNumber}" ></Input>
        <Label text="{i18n>SHOW_INS_NOTES}" class="showLabel"></Label>
    </content>
    <beginButton>
        <Button text="{i18n>OK}" press="saveMaxHit">
        </Button>
    </beginButton>
    <endButton>
        <Button text="{i18n>CANCEL} " press="closeShow">
        </Button>
    </endButton>

</Dialog>
